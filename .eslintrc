{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "prettier",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:import/recommended"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "JSX": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": 0,
    "no-unused-vars": "warn"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "extensions": [".js"],
        "map": [["@", "./src"]]
      }
    }
  }
}
